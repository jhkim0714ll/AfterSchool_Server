server:
  port: number

spring:
  datasource:
    driver-class-name: string
    url: string
    username: string
    password: string

  jpa:
    show-sql: boolean
    generate-ddl: boolean
    hibernate:
      ddl-auto: none

    properties:
      hibernate:
        format_sql: boolean

jwt:
  header: string
  secret: string
  access-token-validity-in-seconds: number